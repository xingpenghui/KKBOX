<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kkbox.mapper.BlacklistMapper">
  <resultMap id="BaseResultMap" type="com.kkbox.domain.Blacklist">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="uid" jdbcType="INTEGER" property="uid" />
    <result column="bid" jdbcType="INTEGER" property="bid" />
  </resultMap>
  <sql id="Base_Column_List">
    id, uid, bid
  </sql>
  <select id="selectById"  resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from t_blacklist
    where id = #{id,jdbcType=INTEGER}
  </select>
    <select id="selectAll"  resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from t_blacklist
    </select>
  <delete id="deleteByUid" parameterType="java.lang.Integer">
    delete from t_blacklist
    where uid = #{uid,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="com.kkbox.domain.Blacklist" useGeneratedKeys="true" keyProperty="id">
    insert into t_blacklist (id, uid, bid)
    values (#{id,jdbcType=INTEGER}, #{uid,jdbcType=INTEGER}, #{bid,jdbcType=INTEGER})
  </insert>

</mapper>
