<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kkbox.mapper.MucollectionMapper">
  <resultMap id="BaseResultMap" type="com.kkbox.domain.Mucollection">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="mid" jdbcType="int" property="mid" />
    <result column="uid" jdbcType="int" property="uid" />
  </resultMap>
  <sql id="Base_Column_List">
    id, mid, uid
  </sql>
  <select id="selectByMid"  resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from t_mucollection
    where mid = #{mid,jdbcType=INTEGER}
  </select>
  <delete id="deleteByMid" parameterType="java.lang.Integer">
    delete from t_mucollection
    where mid = #{mid,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="com.kkbox.domain.Mucollection" useGeneratedKeys="true" keyProperty="id">
    insert into t_mucollection (id, mid, uid)
    values ( #{mid}, #{uid})
  </insert>

</mapper>